name=papirus-icon-theme
version=20210302
release=1
source=("https://github.com/PapirusDevelopmentTeam/papirus-icon-theme/archive/$version.tar.gz::archive=$name-$version.tar.gz")

build() {
	cd "$SRC"/$name-$version
	make DESTDIR="$PKG" install
}
